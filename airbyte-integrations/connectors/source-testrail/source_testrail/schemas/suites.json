{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": ["null", "string"]
    },
    "project_id": {
      "type": "integer"
    },
    "is_master": {
      "type": "boolean"
    },
    "is_baseline": {
      "type": "boolean"
    },
    "is_completed": {
      "type": "boolean"
    },
    "completed_on": {
      "type": ["null", "string"]
    },
    "url": {
      "type": "string"
    }
  }
}